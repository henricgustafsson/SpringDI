package se.lexicon.henric.dependencyinjection.config;

import org.springframework.context.annotation.Bean;
import org.springframework.context.annotation.Configuration;

import se.lexicon.henric.dependencyinjection.util.SystemOutput;
import se.lexicon.henric.dependencyinjection.util.SystemOutputService;

@Configuration
public class OutputConfig {

	@Bean
	public SystemOutputService getOutput(){
		return new SystemOutput();
	}
}
